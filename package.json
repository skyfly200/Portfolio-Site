{
  "name": "Portfolio-Site",
  "version": "3.1.0",
  "description": "Portfolio Website for Skyler Fly-Wilson",
  "author": "Skyler Fly-Wilson",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "coverage": "nyc vue-cli-service test:unit --recursive src/**/*.vue tests/unit/**/*.js && nyc report --reporter=text-lcov > coverage.lcov && codecov -t f8c33280-dce8-4374-a9e1-664447b170c7",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "lodash": "^4.17.11",
    "marked": "^0.5.1",
    "moment": "^2.29.4",
    "vue": "^2.5.17",
    "vue-axios": "^2.1.4",
    "vue-flux": "^5.1.5",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-unit-mocha": "^3.2.0",
    "@vue/cli-service": "^3.1.3",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.1.2",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "file-loader": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "node-sass": "^4.9.0",
    "nyc": "^13.1.0",
    "sass-loader": "^7.0.1",
    "sass-resources-loader": "^2.0.0",
    "sinon": "^7.1.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^1.1.2",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "license": "GPL-3.0",
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "check-coverage": false,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/**/*.{js,vue}",
      "src/*.vue"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "cache": true,
    "all": true
  },
  "repository": "github:skyfly200/Portfolio-Site"
}
